#include <iostream>
using namespace std;
int main()
{
    int n,n1,n2,n3;
    int sum1=0,sum2=0,sum3=0;
    cin>>n;
    while(n--){
        cin>>n1>>n2>>n3;
        sum1+=n1;
        sum2+=n2;
        sum3+=n3;
    }
    if(sum1==0 && sum2==0 && sum3==0)
        cout<<"YES"<<endl;
    else
        cout<<"NO"<<endl;
    return 0;
}
